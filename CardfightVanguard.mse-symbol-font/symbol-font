	image: v_sb_circle.png
	image font size: 155
	code: [0-9]+(?![C])
	regex: yes
	draw text: 0
	text margin left:   .2
	text margin right:  .2
	text margin top:    -.1
	text margin bottom: -.1
	text font:
		name: Hemi Head Rg Bold
		size: 1
		color: rgb(255,255,255)
		max stretch: 0
