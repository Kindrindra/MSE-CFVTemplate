card field:
	type: text
	name: sp_text
	multi line: true
	editable: false
	save value: false
	#default: card.rule_text
	default: remove_color_tags(tag_contents(tag: "<atom-redtext>", contents: { "\"" }, card.rule_text))
	#default: get_sp_text(value: card.rule_text)
	#script: filter_sp_text(input: card.rule_text)

